        "json_list = []\n",
        "for movie in movie_id:\n",
        "    url = basic_url.format(movie, api_key)\n",
        "    r = requests.get(url)\n",
        "    if r.status_code != 200:\n",
        "        continue\n",
        "    else:\n",
        "        data = r.json()\n",
        "        json_list.append(data) \n",
        "df = pd.DataFrame(json_list)"
